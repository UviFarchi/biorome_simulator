{
  "name": "biorome_simulator",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}\" --write",
    "format:check": "prettier \"**/*.{js,jsx,ts,tsx,vue,json,css,scss,md}\" --check",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "mitt": "^3.0.1",
    "pinia": "^3.0.3",
    "uuid": "^11.1.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "jsdom": "^26.1.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
